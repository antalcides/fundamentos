    \pgfkeyssetvalue{/tcb/libload/#1}{}%
    \def\tcb@library@inload{#2}%
    \def\tcb@library@version{-}%
    \edef\tcb@at@catcode{\the\catcode`@}%
    \catcode`\@11\relax%
    \input\tcbpkgprefix#2\relax%
    \catcode`\@\tcb@at@catcode\relax%
    \tcb@check@library@version}{}%
}

